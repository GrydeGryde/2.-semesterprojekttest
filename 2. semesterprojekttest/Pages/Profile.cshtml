@page
@using _2._semesterprojekttest.Models
@model _2._semesterprojekttest.Pages.ProfileModel
@{
    ViewData["Title"] = "Test af Billed Upload";
}
<div class="text-center">
    <h1 class="display-4">Test af billed uploading</h1>
    <table>
        <thead>
        <tr>
            
            <th>Billed</th>
        </tr>
        </thead>
        <tbody>
        @foreach (Picture p in Model.Pictures)
        {
            <tr>
                @{ String file = "data:image/jpeg;base64," + Convert.ToBase64String(p.Picture1);}
                <td style="height: 200px; width: 300px"><img src="@file" /></td>
            </tr>

        }
        </tbody>
    </table>



</div>
